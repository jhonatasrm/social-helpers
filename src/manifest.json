{
	"manifest_version": 2,
	"name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
	"version": "0.1",
	"homepage_url": "https://github.com/jhonatasrm/social-helpers",
	"default_locale": "en",
	"author": "Jhonatas Rodrigues",
	"icons": {
  "48": "res/icons/icon.png",
  "96": "res/icons/icon@2x.png"
	},
	"browser_action": {
		"browser_style": true,
		"default_icon": {
    "16": "../res/icons/social_helpers_dark-16.png",
    "32": "../res/icons/social_helpers_dark-32.png"
  },
		"default_title": "__MSG_extensionName__",
		"default_popup": "../html/popup.html",
		"theme_icons": [{
				"light": "../res/icons/social_helpers_light-16.png",
				"dark": "../res/icons/social_helpers_dark-16.png",
				"size": 16
			},{
				"light": "../res/icons/social_helpers_light-32.png",
				"dark": "../res/icons/social_helpers_dark-32.png",
				"size": 32
				}]
	},
	"permissions": [
		"tabs",
		"storage"
	]
}
